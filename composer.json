{
  "name": "v360tech/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "version": "11.1.0",
  "authors": [
    {
      "name": "Yogesh Kapuriya",
      "email": "it@v360.tech"
    }
  ],
  "require": {
    "php": "^7.0 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "laravel/framework": "^11.0",
    "michaeldrennen/remote-file": "^2.0.3",
    "michaeldrennen/local-file": "^2.0",
    "curl/curl": "^2.2",
    "nesbot/carbon": "^2.28 || ^3.0",
    "symfony/browser-kit": "^7.1",
    "symfony/http-client": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "V360Tech\\Geonames\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "V360Tech\\Geonames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "V360Tech\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}